#include <stdio.h>

int graph[20][20], visited[20];
int queue[20], front = -1, rear = -1;

// BFS Function
void BFS(int start, int n) {
    int i, node;
    for (i = 1; i <= n; i++)
        visited[i] = 0;

    front = rear = -1;

    visited[start] = 1;
    queue[++rear] = start;

    printf("BFS Traversal: ");

    while (front != rear) {
        node = queue[++front];
        printf("%d ", node);

        for (i = 1; i <= n; i++) {
            if (graph[node][i] == 1 && visited[i] == 0) {
                queue[++rear] = i;
                visited[i] = 1;
            }
        }
    }
    printf("\n");
}

// DFS Function
void DFS(int start, int n) {
    int i;
    visited[start] = 1;
    printf("%d ", start);

    for (i = 1; i <= n; i++) {
        if (graph[start][i] == 1 && visited[i] == 0)
            DFS(i, n);
    }
}

int main() {
    int n, i, j, start;

    printf("Enter number of vertices: ");
    scanf("%d", &n);

    printf("Enter adjacency matrix:\n");
    for (i = 1; i <= n; i++) {
        for (j = 1; j <= n; j++) {
            scanf("%d", &graph[i][j]);
        }
    }

    printf("Enter starting node: ");
    scanf("%d", &start);

    // BFS
    BFS(start, n);

    // DFS
    for (i = 1; i <= n; i++)
        visited[i] = 0;

    printf("DFS Traversal: ");
    DFS(start, n);
    printf("\n");

    return 0;
}
